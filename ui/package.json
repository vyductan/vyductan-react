  "type": "module",
  "exports": {
    "./types": "./src/types.ts",
    "./lib/*": "./src/lib/*.ts",
    "./ui/*": "./src/ui/*.tsx",
    "./shadcn/*": "./src/shadcn/*.tsx",
    "./components": "./src/components/index.ts",
    "./components/*": "./src/components/*/index.ts",
    "./layout/*": "./src/layout/*/index.ts",
    "./icons": "./src/icons/index.ts",
